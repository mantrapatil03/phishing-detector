# Environment variables for phishing-detection
# Copy to .env and edit as needed (e.g., for API keys)

# Paths (optional overrides)
DATA_DIR=data
MODELS_DIR=models

# Logging level
LOG_LEVEL=INFO

# API settings
FLASK_ENV=development
FLASK_DEBUG=True

# Optional: Selenium driver path (if using screenshot.py)
SELENIUM_DRIVER_PATH=/usr/local/bin/chromedriver